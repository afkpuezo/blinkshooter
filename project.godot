[application]

config/name="blinkshooter"
run/main_scene="res://scenes/ui/menus/main_menu/main_menu.tscn"

[autoload]

GameEvents="*res://scenes/singletons/game_events.gd"
TargetReticle="*res://scenes/singletons/reticle/reticle.tscn"
Spawner="*res://scenes/singletons/spawner.gd"
EnemyBarManager="*res://scenes/singletons/enemy_bar_manager.gd"
CameraManager="*res://scenes/singletons/camera_manager.gd"
PlayerUI="*res://scenes/ui/player_ui/player_ui.tscn"
LevelLoader="*res://scenes/singletons/level_management/level_loader.gd"
PauseManager="*res://scenes/ui/menus/pause_menu/pause_manager.tscn"
LevelGlobal="*res://scenes/singletons/level_management/level_global.gd"
SaveLoad="*res://scenes/singletons/level_management/saveload.gd"

[debug]

settings/stdout/print_fps=true

[display]

window/size/width=1920
window/size/height=1080
window/size/resizable=false
window/size/fullscreen=true
mouse_cursor/custom_image="res://scenes/singletons/reticle/crosshair.png"
mouse_cursor/custom_image_hotspot=Vector2( 16, 16 )
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

static=false
alias=false
autoload=false
main=false
fps=false
framerate=false

